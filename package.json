{
  "name": "ai-101-ts",
  "publisher": "GoupilJeremy",
  "displayName": "ai-101-ts",
  "description": "VSCode AI agents with transparent HUD",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-101-ts.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "ai-101-ts.applyPreset",
        "title": "Apply Configuration Preset",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.exportConfig",
        "title": "Export Configuration",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.importConfig",
        "title": "Import Configuration",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.toggleFocusMode",
        "title": "Toggle Focus Mode",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.togglePerformanceMode",
        "title": "Toggle Performance Mode",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.toggleLargeText",
        "title": "Toggle Large Text Mode",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.toggleHighContrast",
        "title": "Toggle High Contrast Mode",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.toggleColorblind",
        "title": "Toggle Colorblind Mode",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.focusHud",
        "title": "Focus HUD for Keyboard Navigation",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.addContextFile",
        "title": "Add File to Context",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.removeContextFile",
        "title": "Remove File from Context",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.viewArchitecture",
        "title": "View Detected Architecture",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.setPhase",
        "title": "Set Development Phase",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.acceptSuggestion",
        "title": "Accept AI Suggestion",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.rejectSuggestion",
        "title": "Reject AI Suggestion",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.toggleHUD",
        "title": "Toggle HUD Visibility",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.forceAgentState",
        "title": "Force Agent State (Debug)",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.switchToLearningMode",
        "title": "Switch to Learning Mode",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.switchToExpertMode",
        "title": "Switch to Expert Mode",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.switchToTeamMode",
        "title": "Switch to Team Mode",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.configureApiKeys",
        "title": "Configure API Keys",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.resetConfig",
        "title": "Reset Configuration",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.toggleAgentVisibility",
        "title": "Toggle Agent Visibility",
        "category": "AI 101"
      },
      {
        "command": "ai-101-ts.openDocumentation",
        "title": "Open Documentation",
        "category": "AI 101"
      }
    ],
    "keybindings": [
      {
        "command": "ai-101-ts.toggleHUD",
        "key": "ctrl+alt+h",
        "mac": "cmd+alt+h",
        "when": "editorTextFocus"
      },
      {
        "command": "ai-101-ts.toggleFocusMode",
        "key": "ctrl+shift+alt+f",
        "mac": "cmd+shift+alt+f",
        "when": "editorTextFocus"
      },
      {
        "command": "ai-101-ts.acceptSuggestion",
        "key": "ctrl+enter",
        "mac": "cmd+enter",
        "when": "editorTextFocus"
      },
      {
        "command": "ai-101-ts.rejectSuggestion",
        "key": "ctrl+backspace",
        "mac": "cmd+backspace",
        "when": "editorTextFocus"
      },
      {
        "command": "ai-101-ts.forceAgentState",
        "key": "ctrl+shift+alt+s",
        "mac": "cmd+shift+alt+s",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "AI 101",
      "properties": {
        "ai101.llm.provider": {
          "type": "string",
          "default": "openai",
          "enum": [
            "openai",
            "anthropic",
            "custom"
          ],
          "description": "Select the LLM provider to use."
        },
        "ai101.ui.transparency": {
          "type": "string",
          "default": "medium",
          "enum": [
            "minimal",
            "medium",
            "full"
          ],
          "description": "Set the transparency level of the HUD."
        },
        "ai101.ui.mode": {
          "type": "string",
          "default": "learning",
          "enum": [
            "learning",
            "expert",
            "focus",
            "team",
            "performance"
          ],
          "description": "Set the operating mode of the extension."
        },
        "ai101.performance.maxTokens": {
          "type": "number",
          "default": 4096,
          "description": "Maximum number of tokens to use for context."
        },
        "ai101.telemetry.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable opt-in telemetry for diagnostics."
        },
        "ai101.teamMode.largeText": {
          "type": "boolean",
          "default": false,
          "description": "Enable 125% scale for Team Mode screen sharing visibility."
        },
        "ai101.teamMode.surveyEnabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable team comprehension surveys (opt-in)."
        },
        "ai101.performanceMode.autoActivate": {
          "type": "boolean",
          "default": true,
          "description": "Auto-activate Performance Mode on low-memory systems (<4GB RAM)."
        },
        "ai101.performanceMode.autoSuggest": {
          "type": "boolean",
          "default": true,
          "description": "Suggest Performance Mode when low FPS is detected."
        },
        "ai101.performanceMode.collisionThrottleMs": {
          "type": "number",
          "default": 500,
          "description": "Anti-collision check interval in Performance Mode (ms)."
        },
        "ai101.performanceMode.metricsThrottleMs": {
          "type": "number",
          "default": 1000,
          "description": "Vital Signs Bar update interval in Performance Mode (ms)."
        },
        "ai101.accessibility.highContrast": {
          "type": "boolean",
          "default": null,
          "description": "Enable High Contrast Mode. Set to null for auto-detection based on VSCode theme."
        },
        "ai101.accessibility.autoDetectHighContrast": {
          "type": "boolean",
          "default": true,
          "description": "Automatically enable High Contrast Mode when VSCode High Contrast theme is active."
        },
        "ai101.accessibility.colorblind": {
          "type": "object",
          "properties": {
            "enabled": {
              "type": "boolean",
              "default": false,
              "description": "Enable Colorblind Accessibility Mode"
            },
            "type": {
              "type": "string",
              "enum": [
                "none",
                "deuteranopia",
                "protanopia",
                "tritanopia"
              ],
              "default": "none",
              "description": "Type of color blindness to optimize for"
            }
          },
          "description": "Colorblind accessibility settings"
        },
        "ai101.keyboard.shortcuts": {
          "type": "object",
          "properties": {
            "enableHudNavigation": {
              "type": "boolean",
              "default": true,
              "description": "Enable keyboard navigation for HUD elements"
            },
            "tabNavigation": {
              "type": "string",
              "default": "Tab",
              "description": "Key for tab navigation through HUD elements"
            },
            "shiftTabNavigation": {
              "type": "string",
              "default": "Shift+Tab",
              "description": "Key for reverse tab navigation"
            },
            "activateElement": {
              "type": "string",
              "default": "Enter",
              "description": "Key to activate selected HUD element"
            },
            "dismissElement": {
              "type": "string",
              "default": "Escape",
              "description": "Key to dismiss or close HUD elements"
            },
            "agentNavigation": {
              "type": "string",
              "default": "Arrow Keys",
              "description": "Keys for spatial navigation between agents"
            },
            "alertNavigation": {
              "type": "string",
              "default": "Arrow Keys",
              "description": "Keys for spatial navigation between alerts"
            }
          },
          "description": "Keyboard navigation shortcuts for HUD accessibility"
        },
        "ai101.architecture.overrides": {
          "type": "object",
          "default": {},
          "description": "Override detected project architecture patterns (e.g., techStack, patterns, conventions)."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "js-tiktoken": "^1.0.10",
    "openai": "^4.28.0"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.108.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "axe-core": "^4.8.2",
    "esbuild": "^0.25.12",
    "eslint": "^9.25.1",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3",
    "vitest": "^4.0.17"
  }
}